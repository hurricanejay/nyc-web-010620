
<form action="/users/<%=@user.id%>" method="POST">
<input type="hidden" name="_method" value="PATCH"/>
<input type="text" placeholder="enter user's name" name="user[name]" value="<%=@user.name%>"/>
<input type="number" placeholder="enter user's age" name="user[age]" value="<%=@user.age%>"/>
<input type="submit" value="Submit User"/>
</form>
